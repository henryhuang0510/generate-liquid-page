        <script>
            const previewImageItems = document.querySelectorAll('#preview-image-item');
            const productFeaturedImage = document.getElementById('product-featured-image');
            const buttons = document.querySelectorAll('.slr-container-right-variants-list button');
            const infoBox = document.getElementById('variant-info');
            previewImageItems.forEach(item => {
                item.addEventListener('click', () => {
                    const newSrc = item.src.replace(/(\d+)x/, '1200x');
                    const newSrcSet = `
                        ${item.src.replace(/(\d+)x/, '352x')}   352w,
                        ${item.src.replace(/(\d+)x/, '832x')}   832w,
                        ${item.src.replace(/(\d+)x/, '1200x')} 1200w,
                    `;
                    productFeaturedImage.src = newSrc;
                    productFeaturedImage.srcset = newSrcSet;
                });
            });


            const contentMap = {
                sizing: 'One size fits all.',
                shipping: 'Dispatches within 24-48 hours.',
                wholesale: 'Wholesale options available for bulk orders. Contact us for details.'
            };

            buttons.forEach(button => {
                button.addEventListener('click', () => {
                    buttons.forEach(btn => btn.classList.remove('active'));
                    button.classList.add('active');
                    const type = button.getAttribute('data-type');
                    infoBox.textContent = contentMap[type];
                });
            });


			// 立即购买功能
			document.getElementById('slr-buy-now-btn').addEventListener('click', function() {
				const variantId = getCurrentVariantId();

				fetch('/cart/add.js', {
					method: 'POST',
					headers: { 'Content-Type': 'application/json' },
					body: JSON.stringify({ id: variantId})
				})
						.then(function() {
							window.location.href = '/checkout';
						})
						.catch(function(error) {
							console.error('立即购买失败:', error);
